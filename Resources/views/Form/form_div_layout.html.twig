{% block braincrafted_name_widget %}
{% spaceless %}
    <div {{ block('widget_container_attributes') }}>
        {{ form_errors(form.firstName) }}
        {{ form_errors(form.lastName) }}
        {{ form_widget(form.firstName) }}&nbsp;
        {{ form_widget(form.lastName) }}
    </div>
{% endspaceless %}
{% endblock braincrafted_name_widget %}
